    "def my_map(functs, seq):\n",
    "    res = []\n",
    "    for data in seq:\n",
    "        for f in functs:\n",
    "            data = f(data)\n",
    "        res.append(data)\n",
    "    return res\n",
    "\n",
    "print(my_map([abs, lambda x: x**3], [10,-9,-4,5] ))"
